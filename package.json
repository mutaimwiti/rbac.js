{
  "name": "@mutaimwiti/flex-rbac",
  "version": "0.0.1d",
  "description": "Easy and intuitive role based access control on Express.",
  "main": "./index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --colors",
    "test:watch": "jest --colors --watch",
    "build": "rimraf ./lib; babel -d lib src/",
    "build_and_stage": "yarn build; git add .",
    "lint": "eslint ."
  },
  "pre-commit": [
    "lint",
    "test",
    "build_and_stage"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutaimwiti/flex-rbac.git"
  },
  "keywords": [
    "flex",
    "rbac",
    "flex-rbac",
    "node",
    "nodejs",
    "express"
  ],
  "author": "Mutai Mwiti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mutaimwiti/flex-rbac/issues"
  },
  "homepage": "https://github.com/mutaimwiti/flex-rbac#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "expect": "^24.5.0",
    "jest": "^24.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4"
  },
  "jest": {
    "verbose": true,
    "coverageReporters": [
      "text",
      "lcov",
      "clover"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "index.js"
    ]
  },
  "dependencies": {
    "require-all": "^3.0.0"
  }
}
