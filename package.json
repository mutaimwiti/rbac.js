{
  "name": "xps-rbac",
  "version": "0.1.0-beta.2",
  "description": "Easy and intuitive role-based access control library for Express apps.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest --colors",
    "test:watch": "jest --colors --watch",
    "build": "babel -d lib src/ && cp src/index.d.ts lib/index.d.ts",
    "lint": "eslint .",
    "prepublishOnly": "yarn clean && yarn build && yarn lint && yarn test"
  },
  "pre-commit": [
    "clean",
    "build",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/mutaimwiti/xps-rbac.git"
  },
  "keywords": [
    "xps",
    "rbac",
    "xps-rbac",
    "node",
    "node-rbac",
    "nodejs",
    "nodejs-rbac",
    "express",
    "express-rbac",
    "authorize",
    "express-authorize",
    "authorization",
    "express-authorization"
  ],
  "author": "Mutai Mwiti",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/mutaimwiti/xps-rbac/issues"
  },
  "homepage": "https://gitlab.com/mutaimwiti/xps-rbac#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.4.3",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "expect": "^24.5.0",
    "jest": "^24.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "roots": [
      "tests"
    ],
    "verbose": true,
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "clover"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "index.js"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "require-all": "^3.0.0"
  }
}
